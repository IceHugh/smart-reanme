{
  "name": "smart-rename",
  "version": "0.0.1",
  "main": "./dist/extension.js",
  "devDependencies": {
    "@types/vscode": "^1.92.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "esbuild": "^0.21.5",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  },
  "activationEvents": [],
  "categories": [
    "Other"
  ],
  "contributes": {
    "commands": [
      {
        "command": "smart-rename.rename",
        "title": "SmartRename: Rename Variable",
        "category": "SmartRename"
      },
      {
        "command": "smart-rename.setApiKey",
        "title": "SmartRename: Set API Key",
        "category": "SmartRename"
      },
      {
        "command": "smart-rename.setBaseUrl",
        "title": "SmartRename: Set Base URL",
        "category": "SmartRename"
      },
      {
        "command": "smart-rename.setModel",
        "title": "SmartRename: Set AI Model",
        "category": "SmartRename"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "smart-rename.rename",
          "when": "editorHasSelection",
          "group": "navigation@100"
        }
      ]
    },
    "configuration": {
      "title": "SmartRename Configuration",
      "properties": {
        "smart-rename.openaiApiKey": {
          "type": "string",
          "default": "",
          "description": "Your OpenAI API Key"
        },
        "smart-rename.baseUrl": {
          "type": "string",
          "default": "https://api.openai.com",
          "description": "The base URL for the OpenAI API"
        },
        "smart-rename.model": {
          "type": "string",
          "default": "gpt-4o-mini",
          "description": "The AI model to use for generating names"
        }
      }
    },
    "keybindings": [
      {
        "command": "smart-rename.rename",
        "key": "ctrl+alt+r",
        "when": "editorTextFocus && editorHasSelection",
        "mac": "cmd+alt+r"
      }
    ]
  },
  "description": "",
  "displayName": "SmartRename",
  "engines": {
    "vscode": "^1.92.0"
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  }
}